<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="helloDialog"
		title="Hello">
		<content>
			<core:Icon
				src="sap-icon://hello-world"
				size="8rem"
				class="sapUiMediumMargin"/>
		</content>
		<beginButton>
			<Button
				text="Close"
				press="onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>
